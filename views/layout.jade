	body
		div(id="alerts-div")
			//Make alerts
			-if(alert != undefinied){
			div(class="alert alert-"+alert.type+" alert-dismissible" role="alert")
				button(type="button" class="close" data-dismiss="alert" aria-label="close")
					span(aria-hidden="true") &times
				strong=alert.title
				p=alert.body
			-}
		div(class="page-header")
			block title
		block content
